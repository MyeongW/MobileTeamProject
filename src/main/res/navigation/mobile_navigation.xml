<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home"
    tools:ignore="UnusedNavigation">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.term.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_rain"
        android:name="com.example.term.ui.rain.RainFragment"
        android:label="@string/title_rain"
        tools:layout="@layout/fragment_rain" />

    <fragment
        android:id="@+id/navigation_umb"
        android:name="com.example.term.ui.umb.UmbrellaFragment"
        android:label="@string/title_umb"
        tools:layout="@layout/fragment_umb" />

    <fragment
        android:id="@+id/navigation_alarm"
        android:name="com.example.term.ui.alarm.AlarmFragment"
        android:label="@string/title_alarm"
        tools:layout="@layout/fragment_alarm" />

    <fragment
        android:id="@+id/navigation_set"
        android:name="com.example.term.ui.set.SettingFragment"
        android:label="@string/title_setting"
        tools:layout="@layout/fragment_setting" />
</navigation>
